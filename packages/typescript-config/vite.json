{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Vite",
	"extends": "./base.json",
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"types": ["vite/client"],
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": false,
		"noEmit": true
	}
}
