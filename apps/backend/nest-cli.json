{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "builder": "swc",
    "webpackConfigPath": "./webpack.config.js"
  },
  "projects": {
    "ai": {
      "type": "library",
      "root": "libs/ai",
      "entryFile": "index",
      "sourceRoot": "libs/ai/src",
      "compilerOptions": {
        "tsConfigPath": "libs/ai/tsconfig.lib.json"
      }
    },
    "hashing": {
      "type": "library",
      "root": "libs/hashing",
      "entryFile": "index",
      "sourceRoot": "libs/hashing/src",
      "compilerOptions": {
        "tsConfigPath": "libs/hashing/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "mailer": {
      "type": "library",
      "root": "libs/mailer",
      "entryFile": "index",
      "sourceRoot": "libs/mailer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mailer/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "s3": {
      "type": "library",
      "root": "libs/s3",
      "entryFile": "index",
      "sourceRoot": "libs/s3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/s3/tsconfig.lib.json"
      }
    }
  }
}